{
    "version": 2,
    "builds":[{
        "src": "CV/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime":"python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "CV/wsgi.py"
        },
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src": "app/static",
            "dest": "/app/static/$1"
        }
    ]
}